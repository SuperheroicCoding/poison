{
  "name": "angular-examples",
  "version": "1.3.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "cross-env NODE_OPTIONS=--max_old_space_size=12000 ng serve --prod --port=4300",
    "build-deps": "npm run asbuild && npm run build-thanos",
    "build-thanos": "ng build sc-thanos",
    "build-thanos-prod": "ng build sc-thanos --prod",
    "test-thanos": "ng test sc-thanos",
    "test": "ng test angular-examples",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "asbuild:untouched": "asc projects/fib-wasm/assembly/index.ts -d projects/fib-wasm/index.d.ts -b projects/fib-wasm/build/untouched.wasm -t projects/fib-wasm/build/untouched.wat --sourceMap --importMemory --debug",
    "asbuild:optimized": "asc projects/fib-wasm/assembly/index.ts -d projects/fib-wasm/index.d.ts -b projects/fib-wasm/build/optimized.wasm -t projects/fib-wasm/build/optimized.wat --sourceMap --importMemory --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "ghPages": "npm run asbuild && ng build --prod --output-path docs --named-chunks --base-href https://superheroiccoding.github.io/AngularExamples/",
    "buildS3": "cross-env NODE_OPTIONS=--max_old_space_size=7168 ng build --prod --output-path distS3 --base-href https://angularexamples.superheroiccoding.de",
    "updateFirebase": "node node-scripts/init-firebase",
    "deploy": "npm version patch && npm run build-libs && npm run buildS3 && node node-scripts/sync-S3",
    "deploy-without-build": "node node-scripts/sync-S3",
    "deploy-exclude-assets": "npm version patch && npm run build-deps && npm run buildS3 && node node-scripts/sync-S3 --excludeAssets",
    "postinstall": "ngcc --tsconfig=projects/angular-examples/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.4",
    "@angular/cdk": "10.2.3",
    "@angular/common": "10.1.4",
    "@angular/compiler": "10.1.4",
    "@angular/core": "10.1.4",
    "@angular/elements": "^10.1.4",
    "@angular/fire": "^6.0.3",
    "@angular/forms": "10.1.4",
    "@angular/material": "10.2.3",
    "@angular/platform-browser": "10.1.4",
    "@angular/platform-browser-dynamic": "10.1.4",
    "@angular/router": "10.1.4",
    "@angular/service-worker": "10.1.4",
    "@datorama/akita": "^5.2.4",
    "@datorama/akita-ng-router-store": "^5.1.6",
    "@datorama/akita-ngdevtools": "^5.0.3",
    "@ngneat/until-destroy": "^8.0.2",
    "@tensorflow/tfjs": "2.4.0",
    "ace-builds": "^1.4.12",
    "akita-ng-fire": "^3.1.4",
    "angulartics2": "^10.0.0",
    "brace": "^0.11.1",
    "browserslist": "^4.14.5",
    "core-js": "3.6.5",
    "document-register-element": "^1.14.6",
    "firebase": "^7.21.1",
    "gl-matrix": "3.3.0",
    "gpu.js": "2.10.4",
    "mathjs": "^7.3.0",
    "ngx-ace-editor-wrapper": "^9.0.1",
    "p5": "^1.1.9",
    "rxjs": "6.6.3",
    "stats.js": "^0.17.0",
    "three": "0.121.1",
    "three-orbitcontrols-ts": "^0.1.2",
    "ts-keycode-enum": "^1.0.6",
    "tslib": "^2.0.1",
    "vega": "^5.16.1",
    "vega-embed": "6.12.2",
    "vega-lite": "4.16.8",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular-devkit/build-ng-packagr": "~0.1001.7",
    "@angular-devkit/schematics": "^11.2.11",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "^10.1.4",
    "@angular/language-service": "10.1.4",
    "@types/gl-matrix": "^3.2.0",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/mathjs": "^6.0.5",
    "@types/node": "^14.11.2",
    "@types/p5": "^0.9.1",
    "@types/stats.js": "^0.17.0",
    "@types/three": "0.103.2",
    "assemblyscript": "^0.14.12",
    "aws-sdk": "^2.772.0",
    "axios": "^0.20.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "firebase-admin": "^9.2.0",
    "html2canvas": "^1.0.0-rc.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.1.2",
    "protractor": "~7.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "8.10.2",
    "tslint": "~6.1.3",
    "typescript": "3.9.7"
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": true,
    "basePath": "./projects/angular-examples/src/app/"
  }
}
