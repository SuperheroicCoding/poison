{
  "name": "angular-examples",
  "version": "1.3.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "cross-env NODE_OPTIONS=--max_old_space_size=12000 ng serve --prod --port=4300",
    "build-deps": "npm run asbuild && npm run build-thanos",
    "build-thanos": "ng build sc-thanos",
    "build-thanos-prod": "ng build sc-thanos --prod",
    "test-thanos": "ng test sc-thanos",
    "test": "ng test angular-examples",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "asbuild:untouched": "asc projects/fib-wasm/assembly/index.ts -d projects/fib-wasm/index.d.ts -b projects/fib-wasm/build/untouched.wasm -t projects/fib-wasm/build/untouched.wat --sourceMap --importMemory --validate --debug",
    "asbuild:optimized": "asc projects/fib-wasm/assembly/index.ts -d projects/fib-wasm/index.d.ts -b projects/fib-wasm/build/optimized.wasm -t projects/fib-wasm/build/optimized.wat --sourceMap --importMemory --validate --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "ghPages": "npm run asbuild && ng build --prod --output-path docs --named-chunks --base-href https://superheroiccoding.github.io/AngularExamples/",
    "buildS3": "cross-env NODE_OPTIONS=--max_old_space_size=7168 ng build --prod --output-path distS3 --base-href https://angularexamples.superheroiccoding.de",
    "updateFirebase": "node node-scripts/init-firebase",
    "deploy": "npm version patch && npm run build-libs && npm run buildS3 && node node-scripts/sync-S3",
    "deploy-without-build": "node node-scripts/sync-S3",
    "deploy-exclude-assets": "npm version patch && npm run build-deps && npm run buildS3 && node node-scripts/sync-S3 --excludeAssets",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.2",
    "@angular/cdk": "9.2.1",
    "@angular/common": "9.1.2",
    "@angular/compiler": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/elements": "^9.1.2",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "9.1.2",
    "@angular/material": "9.2.1",
    "@angular/platform-browser": "9.1.2",
    "@angular/platform-browser-dynamic": "9.1.2",
    "@angular/router": "9.1.2",
    "@angular/service-worker": "9.1.2",
    "@datorama/akita": "^4.23.0",
    "@datorama/akita-ng-router-store": "^5.0.0",
    "@datorama/akita-ngdevtools": "^4.0.0",
    "@ngneat/until-destroy": "^7.1.6",
    "@tensorflow/tfjs": "1.7.1",
    "ace-builds": "^1.4.11",
    "akita-ng-fire": "^2.0.0-rc.2",
    "angulartics2": "^8.3.0",
    "brace": "^0.11.1",
    "browserslist": "^4.12.0",
    "core-js": "3.6.4",
    "document-register-element": "^1.14.3",
    "firebase": "^7.14.1",
    "gl-matrix": "3.3.0",
    "gpu.js": "2.9.3",
    "hammerjs": "^2.0.8",
    "mathjs": "^6.6.2",
    "ng2-ace-editor": "0.3.9",
    "p5": "^1.0.0",
    "rxjs": "6.5.5",
    "stats.js": "^0.17.0",
    "three": "0.115.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "ts-keycode-enum": "^1.0.6",
    "tslib": "^1.11.1",
    "vega": "^5.10.1",
    "vega-embed": "6.6.0",
    "vega-lite": "4.11.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.2",
    "@angular-devkit/build-ng-packagr": "~0.901.2",
    "@angular-devkit/schematics": "^9.1.2",
    "@angular/cli": "9.1.2",
    "@angular/compiler-cli": "^9.1.2",
    "@angular/language-service": "9.1.2",
    "@types/gl-matrix": "^2.4.5",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/mathjs": "^6.0.5",
    "@types/node": "^13.13.2",
    "@types/p5": "^0.9.0",
    "@types/stats.js": "^0.17.0",
    "@types/three": "0.103.2",
    "assemblyscript": "^0.9.4",
    "aws-sdk": "^2.660.0",
    "axios": "^0.19.2",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.2",
    "firebase-admin": "^8.10.0",
    "html2canvas": "^1.0.0-rc.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "ng-packagr": "^9.1.0",
    "protractor": "5.4.3",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "8.8.1",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": true,
    "basePath": "./projects/angular-examples/src/app/"
  }
}
